pipeline{
	agent any
stages {
    stage('Build')
    {
		steps{
        echo "Building the project..."
		}
    }
    
    stage('Deploy')
    {
		steps{
        echo "Deploying the project..."
		}
    }
    
    stage('Test')
    {
		steps{
        echo "Test the project..."
		}
    }
    
    stage('Release')
    {
		steps{
        echo "Released the project..."
		}
    }
} 
}
